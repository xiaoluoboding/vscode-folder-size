		"watch": "tsc -watch -p ./",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/node": "^14.6.3",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
